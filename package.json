{
  "name": "goerr",
  "version": "2.0.0",
  "description": "handle errors in a better way.",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Siwei LU <me@siwei.lu>",
  "license": "MIT",
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "clean": true
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/siwei-lu/goerr"
  },
  "keywords": [
    "error",
    "handling",
    "node",
    "golang"
  ]
}
